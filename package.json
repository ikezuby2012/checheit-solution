{
  "name": "template2",
  "version": "1.0.0",
  "description": "landing page for a new template",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss public/css/style.css -w",
    "devServer": "live-server",
    "start": "node server.js",
    "serve": "npm run-all devServer watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon.font css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style-compressed",
    "build:css": "npm run-all compile:sass concat:css prefix:css compress:css",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "server": "nodemon server.js",
    "server:prod": "cross-env NODE_ENV=production nodemon server.js"
  },
  "author": "mr zuby",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "core-js": "^2.6.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "html-to-text": "^7.0.0",
    "jquery": "^3.4.1",
    "mongoose": "^5.9.15",
    "nodemailer": "^6.4.17",
    "nodemon": "^2.0.6",
    "pug": "^3.0.0",
    "sendgrid": "^5.2.3",
    "validator": "^13.1.1"
  },
  "devDependencies": {
    "node-sass": "^4.13.1",
    "parcel-bundler": "^1.12.3"
  }
}
